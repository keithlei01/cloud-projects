{
  "name": "cognito-dummy-users",
  "version": "0.1.0",
  "description": "AWS CDK TypeScript application that provisions 3 dummy users in an Amazon Cognito User Pool",
  "bin": {
    "cognito-dummy-users": "bin/cognito-dummy-users.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest test/cognito-dummy-users.test.ts",
    "test:integration": "jest test/cognito-dummy-users.integration.test.ts --runInBand --detectOpenHandles --testTimeout=600000",
    "test:integration:full": "./scripts/run-integration-tests.sh all",
    "test:integration:shell": "./scripts/run-integration-tests.sh shell",
    "test:integration:jest": "./scripts/run-integration-tests.sh jest",
    "test:integration:parallel": "PARALLEL=true ./scripts/run-integration-tests.sh all",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:run": "./scripts/run-tests.sh",
    "test:validate": "ts-node test/validate-deployment.ts",
    "integration-test": "./scripts/integration-test.sh",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "aws-cdk": "2.70.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.864.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.865.0",
    "aws-cdk-lib": "2.70.0",
    "constructs": "^10.0.0"
  }
}
